{
  "name": "@lemma/fn-example",
  "version": "0.0.1",
  "description": "Example function for FaaS service",
  "author": "inhibitor <inhibitor@kaist.ac.kr>",
  "private": true,
  "packageManager": "yarn@3.2.2",
  "dependencies": {
    "@lemma/fx": "workspace:^"
  },
  "devDependencies": {
    "@lemma/aws-lambda": "workspace:^",
    "@types/aws-lambda": "^8.10.109",
    "@types/node": "^18.7.8",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^3.0.7"
  },
  "scripts": {
    "build": "vite build",
    "deploy:local": "yarn build && aws-lambda deploy --functionName fn-example",
    "invoke:local": "aws-lambda invoke --functionName fn-example",
    "logs:local": "aws-lambda logs --functionName fn-example"
  }
}
